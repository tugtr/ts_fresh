{
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json",
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
           
        }
    },
    "rules": {
        "indent": ["warn", 4],
        "quotes": ["warn", "double"],
        "no-console":"warn",
        "@typescript-eslint/rule-name": "error",
        "@typescript-eslint/restrict-plus-operands": "error"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "airbnb-base",
        "plugin:@typescript-eslint/recommended",
        "prettier/@typescript-eslint"
    ]
}